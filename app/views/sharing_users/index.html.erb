<h1>Listing sharing_users</h1>

<table>
  <tr>
    <th>Friend</th>
    <th>Downloaded</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sharing_users.each do |sharing_user| %>
  <tr>
    <td><%= sharing_user.friend.name %> <%= sharing_user.friend.email %></td>
    <td><%= sharing_user.downloaded %></td>
    <td><%= link_to 'Show', [@sharing, sharing_user] %></td>
    <td><%= link_to 'Edit', edit_sharing_sharing_user_path(@sharing, sharing_user) %></td>
    <td><%= link_to 'Destroy', [@sharing, sharing_user], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Sharing user', new_sharing_sharing_user_path(@sharing) %>
